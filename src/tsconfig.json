    "app/app.component.ts",
    "app/app.routes.ts",
    "app/components/details/chemicalEquation/chemical-equation.component.ts",
    "app/components/details/chemicalEquation/index.ts",
    "app/components/details/details.routes.ts",
    "app/components/details/metaboliteDetails/index.ts",
    "app/components/details/metaboliteDetails/metabolite-details.component.ts",
    "app/components/details/reactionDetails/index.ts",
    "app/components/details/reactionDetails/reaction-details.component.ts",
    "app/components/search/search-bar/index.ts",
    "app/components/search/search-bar/search-bar.component.spec.ts",
    "app/components/search/search-bar/search-bar.component.ts",
    "app/components/search/search-page/index.ts",
    "app/components/search/search-page/search-page.component.spec.ts",
    "app/components/search/search-page/search-page.component.ts",
    "app/components/search/search-result/index.ts",
    "app/components/search/search-result/search-result.component.spec.ts",
    "app/components/search/search-result/search-result.component.ts",
